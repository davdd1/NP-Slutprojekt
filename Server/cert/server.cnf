[ req ]
default_bits        = 2048
prompt              = no
default_md          = sha256
distinguished_name  = req_distinguished_name
req_extensions      = req_ext

[ req_distinguished_name ]
C                   = SE
ST                  = Stockholm
O                   = STI
CN                  = server

[ req_ext ]
subjectAltName      = @alt_names

[ alt_names ]
DNS.1               = server
DNS.2               = go-server
DNS.3               = localhost
IP.1                = 127.0.0.1
